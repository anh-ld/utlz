{
  "name": "ushort",
  "version": "0.0.0",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20221111.1",
    "prettier": "^2.8.0",
    "typescript": "^4.9.3",
    "wrangler": "2.1.10"
  },
  "private": true,
  "scripts": {
    "start": "wrangler dev",
    "deploy": "wrangler publish",
    "format": "npx prettier --write ."
  },
  "dependencies": {
    "worktop": "^0.7.3"
  },
  "prettier": {
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "htmlWhitespaceSensitivity": "ignore",
    "insertPragma": false,
    "jsxSingleQuote": false,
    "printWidth": 80,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "requirePragma": false,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": false
  }
}
